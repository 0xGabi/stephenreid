<% SocialNetwork.all(sort: { "Order" => "asc" }).each { |social_network| url = social_network['URL'] %>
  <div class="col-auto">
    <a style="text-decoration:none" class="text-white" data-toggle="tooltip" title="<%=url.starts_with?('http') ? url.split('/')[3..-1].join('/')  : url%>" target="_blank" href="<%=url%>">
      <%=social_network['Name']%>
    </a>                  
  </div>                  
<% } %>